{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel",
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": ["Error", "Array"]
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./resources/assets/js"],
        "alias": {
          "~": "./resources/assets/js"
        }
      }
    ]
  ]
}
